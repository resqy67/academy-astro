---
const heading = ["Materi", "Tempat", "Durasi", "Tanggal", "Status"];
const response = await fetch('https://api.apispreadsheets.com/data/cqNmVxsmPKIhbPiv/');
const dataa = await response.json();
const data = dataa.data;
console.log(data);
---

<div class="relative overflow-x-auto">
  <table class="w-full text-sm text-left text-gray-700 dark:text-gray-400">
    <thead
      class="text-xs text-gray-700 uppercase bg-gray-50 dark:bg-gray-700 dark:text-gray-400"
    >
      {
        heading.map((item) => (
          <th scope="col" class="px-6 py-3">
            {item}
          </th>
        ))
      }
    </thead>
    <tbody>
      {
        Array.isArray(data) && data.map((item: {materi: string; tempat: string, waktu: string, tanggal: string, status: string}) => (
          <tr class="bg-white border-b dark:bg-gray-800 dark:border-gray-700">
            <th
              scope="row"
              class="px-6 py-4 font-medium text-gray-900 whitespace-nowrap dark:text-white"
            >
              {item.materi}
            </th>
            <td class="px-6 py-4">{item.tempat}</td>
            <td class="px-6 py-4">{item.waktu}</td>
            <td class="px-6 py-4">{item.tanggal}</td>
            <td class="px-6 py-4">{item.status}</td>
          </tr>
        ))
      }
    </tbody>
  </table>
</div>
